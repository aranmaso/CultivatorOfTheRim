<Defs>

	<TraitDef>
		<defName>CTR_MediocreTalent</defName>
		<commonality>1.0</commonality>
		<degreeDatas>
			<li>
				<label>Mediocre Talent</label>
				<description>{PAWN_nameDef} has mediocre talent in cultivation. not too bad. but not good either.</description>
				<statFactors>
					<CultivationSpeed>0.9</CultivationSpeed>
					<TechniqueEfficiency MayRequire="zomuro.itssorcery">0.9</TechniqueEfficiency>
				</statFactors>
				
			</li>
		</degreeDatas>
		<conflictingTraits>
			<li>CTR_GoodTalent</li>
			<li>CTR_CultivationProdigy</li>
		</conflictingTraits>
	</TraitDef>
	
	<TraitDef>
		<defName>CTR_GoodTalent</defName>
		<commonality>0.5</commonality>
		<degreeDatas>
			<li>
				<label>Good Talent</label>
				<description>{PAWN_nameDef} has good talent in cultivation. they cultivate somewhat faster than other</description>
				<statFactors>
					<CultivationSpeed>1.1</CultivationSpeed>
					<TechniqueEfficiency MayRequire="zomuro.itssorcery">1.1</TechniqueEfficiency>
				</statFactors>				
			</li>
		</degreeDatas>
		<conflictingTraits>
			<li>CTR_MediocreTalent</li>
			<li>CTR_CultivationProdigy</li>
		</conflictingTraits>
	</TraitDef>
	
	<TraitDef>
		<defName>CTR_CultivationProdigy</defName>
		<commonality>0.3</commonality>
		<degreeDatas>
			<li>
				<label>Cultivation Prodigy</label>
				<description>{PAWN_nameDef} is a prodigy in cultivation, they cultivate a lot faster than other</description>
				<statFactors>
					<CultivationSpeed>1.25</CultivationSpeed>
					<TechniqueEfficiency MayRequire="zomuro.itssorcery">1.25</TechniqueEfficiency>
				</statFactors>				
			</li>
		</degreeDatas>
		<conflictingTraits>
			<li>CTR_MediocreTalent</li>
			<li>CTR_GoodTalent</li>
		</conflictingTraits>
	</TraitDef>
	
	<TraitDef>
		<defName>CTR_InnateAlchemistTalent</defName>
		<commonality>0.3</commonality>
		<degreeDatas>
			<li>
				<label>Alchemist Physique</label>
				<description>{PAWN_nameDef} born with a body naturally made for alchemy. they have higher success chance in alchemy than other</description>
				<statOffsets>
					<AlchemySuccessChance>0.25</AlchemySuccessChance>
				</statOffsets>		
				<skillGains>
				<Medicine>5</Medicine>
				</skillGains>
			</li>
		</degreeDatas>
		<conflictingTraits>
		</conflictingTraits>
	</TraitDef>
	

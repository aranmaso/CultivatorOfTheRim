<Defs>

	<SoundDef>
		<defName>CTR_ChronoStele</defName>  
		<context>MapOnly</context>
		<eventNames />  
		<maxSimultaneous>1</maxSimultaneous>  
		<subSounds>
		<li>
			<grains>
				<li Class="AudioGrain_Clip">
				<clipPath>Effects/ClockTicking</clipPath>
			</li>
			</grains>      
			<pitchRange>
				<min>0.83</min>        
				<max>1</max>
			</pitchRange>
			<volumeRange>
				<min>15</min>        
				<max>20</max>
			</volumeRange>    
		</li>
		</subSounds>
	</SoundDef>
	
	<SoundDef>
		<defName>CTR_SwordSlice</defName>  
		<context>MapOnly</context>
		<eventNames />  
		<maxSimultaneous>1</maxSimultaneous>  
		<subSounds>
		  <li>
			<grains>
			  <li Class="AudioGrain_Clip">
				<clipPath>Effects/shwing</clipPath>
			  </li>
			</grains>      
			<pitchRange>
			  <min>0.83</min>        
			  <max>1.15</max>
			</pitchRange>
			<volumeRange>
			  <min>15</min>        
			  <max>20</max>
			</volumeRange>    
		  </li>
		</subSounds>
	</SoundDef>
	
	<SoundDef>
		<defName>CTR_PaperFlip</defName>  
		<sustain>True</sustain>  
		<sustainFadeoutTime>0.15</sustainFadeoutTime>
		<context>MapOnly</context>
		<maxSimultaneous>3</maxSimultaneous>  
		<priorityMode>PrioritizeNearest</priorityMode>  
		<subSounds>
			<li>
				<muteWhenPaused>True</muteWhenPaused>      
				<grains>
					<li Class="AudioGrain_Folder">
						<clipFolderPath>Interact/Work/Research</clipFolderPath>
					</li>
				</grains>      
				<volumeRange>30~50</volumeRange>      
				<pitchRange>0.8~1.2</pitchRange>      
			</li>
		</subSounds>
	</SoundDef>
	
</Defs>